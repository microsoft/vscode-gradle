        },
        {
          "command": "gradle.refresh",
          "when": "gradle:extensionActivated"
        },
        {
          "command": "gradle.runBuild",
          "when": "gradle:extensionActivated"
        },
        {
          "command": "gradle.findTask",
          "when": "gradle:extensionActivated"
        },
        {
          "command": "gradle.createProject",
          "when": "!java:projectManagerActivated"
        },
